{
  "version": 2,
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "build": {
    "commands": [
      "npm install", 
      "npx prisma generate", 
      "npm run build" 
    ]},
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "DELETE"
          ]
      }
  ]
  }
