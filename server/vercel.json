{
    "version": 2,
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "commands": [
        "npm install",
        "npx prisma generate",
        "npm run build"
      ],
      "outputDirectory": "dist"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "functions": {},
    "env": {}
   }
   
