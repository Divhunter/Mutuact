{
    "version": 2,
    "buildCommand": "prisma generate && nest build",
    "installCommand": "npm install",
    "builds": [
    {
    "src": "src/main.js",
    "use": "@vercel/node"
    },
    {
    "src": "dist/**",
    "use": "@vercel/static"
    }
    ],
    
    "routes": [
      
        {
            "src": "/api/(.*)",
            "dest": "src/main.js"
        },
        {
            "src": "/",
            "dest": "dist/src/main.html"
        },
        {
            "src": "/(.+)",
            "dest": "dist/$1"
        }
    ]
    }